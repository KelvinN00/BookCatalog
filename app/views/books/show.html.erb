<h2><%= @book.title %></h2>
<h3><%= @book.author %></h3>
<p><%= @book.description %></p>
<h3><%= @book.isbn %><h3>
<p><%= @book.created_at.strftime("%B %d %Y, %l:%M%P") %></p>


<%= link_to "Back", root_path%>
<%= link_to "Edit", edit_book_path(@book) %>
<%= link_to "Delete", book_path(@book), method: :delete, data: { confirm: "Really delete this book?" } %>



